local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Key System",
    SubTitle = "by StarBurst7266",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Darker",
    MinimizeKey = Enum.KeyCode.RightShift -- Used when theres no MinimizeKeybind
})

-- Fluent provides Lucide Icons, they are optional
local Tabs = {
    KeySystem = Window:AddTab({ Title = "Key System", Icon = "lock" }),
}

_G.Key = "EfBQkzwrFioQUHuukmyyjTwC7N7ruGet"
_G.KeyInput = "string"

local Input = Tabs.KeySystem:AddInput("Input", {
    Title = "Enter Key",
    Description = "Enter key and press enter to verify.",
    Default = "",
    Placeholder = "Key",
    Numeric = false, -- Only allows numbers
    Finished = false, -- Only calls callback when you press enter
    Callback = function(Value)
        _G.KeyInput = Value
        if _G.KeyInput == _G.Key then
            Fluent:Destroy()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/CoderDude1232/GlobalHub/refs/heads/main/GlobalHub"))()
        else
            Window:Dialog({
                Title = "Key System",
                Content = "The key you entered is incorrect.",
                Buttons = {
                    { 
                        Title = "OK",
                        Callback = function()
                            print("Confirmed the dialog.")
                        end 
                    }, {
                        Title = "Cancel",
                        Callback = function()
                            print("Cancelled the dialog.")
                        end 
                    }
                }
            })
        end
    end
})

Window:SelectTab(1)
